var i = 0;

{
    var i = 2;
    print "Should be 2";
    print i;

    {
        i = i + 1;
        print "Should be 3";
        print i;

        var i = 42;

        {
            print "Should be 42";
            {
                print i;
            }
        }
    }

}

print "Should be 0";
print i;
var test = 10 + 2;
var test2 = test + 2;
test2 = 2;

var test3 = 5;

println(test2);
println(test3);

var res = test2 + test3;
println(res);
println(res == 7);

var a = res = test = 555.55000001;
println(res);
println(test);
println(a);

println(true and true);
println(true and false);
println(true or false);
println(false and false);
println(false or false);
println(false or (1 == 1) and (1 < 2));

var i = 1;

i++;
println(i);
println(i++);
println(i);

i--;
println(i);
println(i--);
println(i);

var b = true;
println(!b);
println(!b == !true);
println(1 != 2);
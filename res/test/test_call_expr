var add = fun a, b -> a + b;

println(add(1, 2));
println(add(3, 2));
println(add(add(1,2), add(-1, -3)));

// println(add(1)));
// Just a comment

println(add("Hello ", "World"));

var even = fun n -> if n % 2 == 0 { 2 + 2; true; } else { return false; };

{
    {
        println(even(2));
    }
    println(even(3));
    println(even(42));
}

even = fun n -> n % 2 == 0;

println(even(2));
println(even(3));
println(even(42));

var encapsulated = fun a, b -> add(a, b);
println(encapsulated(40, 2));

var hello = fun -> "Hello";
println(hello());

var fff = fun -> fun -> fun -> "Hello World";

var res = fff()()();
println(res);
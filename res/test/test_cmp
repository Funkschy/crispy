val d1 = 5;
val d2 = 6;

println(d1 == d2);
println(d1 != d2);
println(d1 < d2);
println(d1 > d2);
println(d1 <= d2);
println(d1 >= d2);

println(5 <= 5);

println(true == true);
println(true == false);
println(true < false);
println(true > false);

val s1 = "ABC";
val s2 = "ABD";

println(s1 == s2);
println(s1 != s2);
println(s1 <= s2);
println(s1 >= s2);
println(s1 < s2);
println(s1 > s2);

val s3 = "ABC";

println(s3 == s1);

println(nil == nil)
println(nil != nil)
println("" != nil)
println(nil != "")
println(1 != nil)
println(nil != 1)